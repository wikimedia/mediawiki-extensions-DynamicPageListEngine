	"DpleFeatureCategory": "DpleFeatureCategory.php",
	"DpleFeatureCheck": "DpleFeatureCheck.php",
	"DpleFeatureContains": "DpleFeatureContains.php",
	"DpleFeatureCount": "DpleFeatureCount.php",
	"DpleFeatureExtra": "DpleFeatureExtra.php",
	"DpleFeatureExtrax": "DpleFeatureExtrax.php",
	"DpleFeatureImagecontainer": "DpleFeatureImagecontainer.php",
	"DpleFeatureImageused": "DpleFeatureImageused.php",
	"DpleFeatureLinksBase": "DpleFeatureLinksBase.php",
	"DpleFeatureLinksto": "DpleFeatureLinksto.php",
	"DpleFeatureLinksfrom": "DpleFeatureLinksfrom.php",
	"DpleFeatureNamespace": "DpleFeatureNamespace.php",
	"DpleFeatureOrder": "DpleFeatureOrder.php",
	"DpleFeatureRedirects": "DpleFeatureRedirects.php",
	"DpleFeatureRedirectsto": "DpleFeatureRedirectsto.php",
	"DpleFeatureResults": "DpleFeatureResults.php",
	"DpleFeatureSubpages": "DpleFeatureSubpages.php",
	"DpleFeatureTitlematch": "DpleFeatureTitlematch.php",
	"DpleFeatureUser": "DpleFeatureUser.php",
	"DpleFeatureUses": "DpleFeatureUses.php",
	"DpleFeatureUsedby": "DpleFeatureUsedby.php",
	"DpleQuery": "DpleQuery.php",
	"DpleUtils": "DpleUtils.php",
